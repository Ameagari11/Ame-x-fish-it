    Title = "Complete Delay",
    Desc = "set the delay to complete fishing.",
    Step = 0.01,
    Value = {
        Min = 0.001,
        Max = 5,
        Default = getgenv().MinigameCompleteDelayCasting or 1.00
    },
    Callback = function(value)
        getgenv().MinigameCompleteDelayCasting = value
    end
})


